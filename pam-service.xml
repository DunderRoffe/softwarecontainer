<?xml version="1.0" encoding="UTF-8" ?>
<node name="/com/pelagicore/PAM">
	<interface name="com.pelagicore.PAM">
		<!-- Echo(string input): returns the value given as input !-->
		<method name="Echo">
			<arg direction="in" type="s" name="argument"/>
			<arg direction="out" type="s"/>
		</method>
		<!-- RegisterClient(string cookie, string appId):
			This method wraps PlatformAccessManager::registerClient
			available in platformaccessmanager.h !-->
		<method name="RegisterClient">
			<arg direction="in" type="s" name="cookie"/>
			<arg direction="in" type="s" name="appId"/>
		</method>
		<!-- UnregisterClient(string appId):
			This method wraps PlatformAccessManager::unregisterClient
			available in platformaccessmanager.h !-->
		<method name="UnregisterClient">
			<arg direction="in" type="s" name="appId"/>
		</method>
		<method name="UpdateFinished">
		</method>
	</interface>
</node>
